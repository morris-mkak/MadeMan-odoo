<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_mademan_charge_form" model="ir.ui.view">
        <field name="name">mademan.charge.form</field>
        <field name="model">mademan.charge</field>
        <field name="arch" type="xml">
            <form string="Charge">
                <sheet>
                    <group>
                        <field name="visit_id"/>
                        <field name="amount"/>
                        <field name="description"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
