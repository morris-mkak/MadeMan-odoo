<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_mademan_payment_form" model="ir.ui.view">
        <field name="name">mademan.payment.form</field>
        <field name="model">mademan.payment</field>
        <field name="arch" type="xml">
            <form string="paymment">
                <sheet>
                    <group>
                        <field name="visit_id"/>
                        <field name="amount"/>
                        <field name="payment_date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
